<%@ page import="java.util.Calendar"%><%@ page import="java.text.SimpleDateFormat"%><%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><!DOCTYPE html><html><head><title>ユーザ登録</title><meta name="viewport" content="width=device-width, initial-scale=1"><link rel="stylesheet" href="/libraryWeb/css/bootstrap.min.css"><link href="/libraryWeb/css/library.css" rel="stylesheet" type="text/css" /><script src="/libraryWeb/js/jquery-3.1.0.min.js"></script></head><body class="drawer drawer--left"> <div class="container-fluid">  <h4 class="head-margin">管理ユーザ登録</h4>  <%  	SimpleDateFormat sdf = new SimpleDateFormat("yyyy/MM/dd");        Calendar c = Calendar.getInstance();  %>  <form action=" /Management" method="post">   <div class="form-group">    <label for="InputName">名前</label> <input type="text" name="m_u_name" class="form-control" id="InputName"     placeholder="名前を入力して下さい。" required>   </div>   <div class="form-group">    <label for="InputPassword">パスワード</label> <input type="password" name="m_u_pass" class="form-control"     id="InputPassword" placeholder="パスワードを入力して下さい。" required>   </div>   <div class="form-group">    <label for="InputTextarea">登録日</label> <input type="text" class="form-control" id="InputTextarea" name="u_date"     value="<%=sdf.format(c.getTime())%>" placeholder="要件を入力して下さい。" readonly />   </div>   <input class="btn btn-primary" type="submit" name="submit" value="登録" /> <input class="btn btn-primary"    value="戻る" type="button" onClick="history.back()">  </form> </div></body></html>